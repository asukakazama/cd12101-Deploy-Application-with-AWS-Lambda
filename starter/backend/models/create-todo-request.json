{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "todo",
  "type": "object",
  "properties": {
    "todoId": {
      "type": "string"
    },
    "userId": {
      "type": "string"
    },    
    "createdAt": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "dueDate": {
      "type": "string"
    },
    "done": {
      "type": "boolean",
      "default": false
    },
    "attachmentUrl": {
      "type": "string"
    }            
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false
}